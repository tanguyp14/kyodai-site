  "name": "ng1-package",
  "version": "1.0.0",
  "description": "NEED DEPENDENCIES",
  "main": "index.js",
  "scripts": {
    "dev": "sass --watch --update --style=expanded assets/scss:assets/css",
    "sass-prod": "sass --no-source-map --style=compressed assets/scss:assets/css",
    "prefixer": "postcss --use autoprefixer -o \"assets/css/style.css\" \"assets/css/style.css\"",
    "prod": "npm run sass-prod && npm run prefixer"
